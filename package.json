{
  "name": "carbon-telegram-bot",
  "version": "0.2.0",
  "description": "Telegram Chatbot that generates carbon.now.sh images from source code",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "test": "mocha -r tsnode/register -b test/**/*.test.ts --no-warnings",
    "build:watch": "tsc -w",
    "build": "tsc",
    "build:clean": "rm -rf tsconfig.tsbuildinfo && rm -rf dist && npm run build",
    "start:debug": "concurrently -n tsc,nodemon -i --default-input-target nodemon \"tsc -w\" \"nodemon dist/index.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/rjmunhoz/carbon-telegram-bot.git"
  },
  "keywords": [
    "telegram",
    "carbon",
    "syntax",
    "hightlighing",
    "development",
    "presentation"
  ],
  "author": "Rog√©rio Munhoz <hello@rjmunhoz.me>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/rjmunhoz/carbon-telegram-bot/issues"
  },
  "homepage": "https://github.com/rjmunhoz/carbon-telegram-bot#readme",
  "dependencies": {
    "@sentry/integrations": "^5.6.1",
    "@sentry/node": "^5.6.2",
    "@types/node": "^12.7.2",
    "ngrok": "^3.2.5",
    "puppeteer": "^1.19.0",
    "sugar-env": "^1.5.7",
    "telegraf": "^3.32.0"
  },
  "devDependencies": {
    "@types/puppeteer": "^1.19.1",
    "concurrently": "^4.1.2",
    "typescript": "^3.5.3"
  },
  "peerDependencies": {
    "nodemon": "^1.19.2"
  }
}
